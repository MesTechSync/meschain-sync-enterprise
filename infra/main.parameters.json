{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME=prod}"
    },
    "location": {
      "value": "${AZURE_LOCATION=westeurope}"
    },
    "projectName": {
      "value": "meschain-sync"
    },
    "adminEmail": {
      "value": "admin@meschain-sync.com"
    },
    "mysqlAdminUser": {
      "value": "meschain_admin"
    },
    "mysqlAdminPassword": {
      "value": "${MYSQL_ADMIN_PASSWORD}"
    },
    "jwtSecret": {
      "value": "${JWT_SECRET}"
    },
    "signalrSecret": {
      "value": "${SIGNALR_SECRET}"
    }
  }
}
