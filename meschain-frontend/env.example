# MesChain-Sync Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# Trendyol API Configuration
REACT_APP_TRENDYOL_API_KEY=your_trendyol_api_key_here
REACT_APP_TRENDYOL_API_SECRET=your_trendyol_api_secret_here
REACT_APP_TRENDYOL_SUPPLIER_ID=your_supplier_id_here
REACT_APP_TRENDYOL_API_BASE=https://api.trendyol.com
REACT_APP_TRENDYOL_ENV=sandbox

# Backend API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080/admin/extension/module/meschain
REACT_APP_API_TOKEN=your_backend_api_token

# Production API Configuration (for production deployment)
REACT_APP_PROD_API_URL=https://your-domain.com/admin/extension/module/meschain
REACT_APP_PROD_WS_URL=wss://your-domain.com/ws

# Staging API Configuration (for staging deployment)
REACT_APP_STAGING_API_URL=https://staging.your-domain.com/admin/extension/module/meschain
REACT_APP_STAGING_WS_URL=wss://staging.your-domain.com/ws

# WebSocket Configuration
REACT_APP_WEBSOCKET_URL=ws://localhost:8080/ws
REACT_APP_WEBSOCKET_RECONNECT_INTERVAL=5000

# CDN Configuration (optional)
REACT_APP_CDN_URL=https://cdn.your-domain.com

# PWA Configuration
REACT_APP_PWA_ENABLED=true
REACT_APP_NOTIFICATION_ENABLED=true

# Development Configuration
REACT_APP_DEBUG_MODE=true
REACT_APP_CACHE_ENABLED=true
REACT_APP_CACHE_DURATION=300000

# Analytics Configuration
REACT_APP_ANALYTICS_ENABLED=false
REACT_APP_ANALYTICS_ID=

# Error Reporting Configuration
REACT_APP_ERROR_REPORTING_ENDPOINT=https://your-error-reporting-service.com/api
REACT_APP_ERROR_REPORTING_API_KEY=your_error_reporting_api_key

# Performance Monitoring
REACT_APP_BUNDLE_ANALYSIS=false

# Feature Flags
REACT_APP_FEATURE_DROPSHIPPING=true
REACT_APP_FEATURE_MULTI_MARKETPLACE=true
REACT_APP_FEATURE_ADVANCED_ANALYTICS=true
REACT_APP_FEATURE_REAL_TIME_SYNC=true 