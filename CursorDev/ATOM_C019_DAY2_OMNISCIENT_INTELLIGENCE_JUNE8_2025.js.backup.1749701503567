    /**
     * üìä Performance metriklerini g√ºnceller
     */
    updatePerformanceMetrics(metric, increment) {
        if (metric === 'processingSpeed') {
            this.performanceMetrics.processingSpeed += increment;
        } else if (metric === 'aiAccuracy') {
            this.performanceMetrics.aiAccuracy = Math.min(99.99, this.performanceMetrics.aiAccuracy + increment);
        }
    }

    /**
     * üß† Omniscience level'ƒ± g√ºnceller
     */
    updateOmniscienceLevel(increment) {
        this.performanceMetrics.omniscienceLevel = Math.min(100, this.performanceMetrics.omniscienceLevel + increment);
        this.performanceMetrics.universalKnowledge = Math.min(100, this.performanceMetrics.universalKnowledge + increment * 0.8);
    }

