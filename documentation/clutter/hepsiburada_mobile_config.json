{
  "mobile_optimization": {
    "responsive_breakpoints": {
      "mobile": "max-width: 768px",
      "tablet": "768px - 1024px",
      "desktop": "min-width: 1024px"
    },
    "touch_interface": {
      "gesture_support": true,
      "swipe_navigation": true,
      "pinch_zoom": true,
      "touch_feedback": true
    },
    "performance": {
      "lazy_loading": true,
      "image_compression": true,
      "cache_optimization": true,
      "offline_support": true
    },
    "mobile_payments": {
      "apple_pay": true,
      "google_pay": true,
      "mobile_banking": true,
      "qr_code_payments": true
    }
  }
}