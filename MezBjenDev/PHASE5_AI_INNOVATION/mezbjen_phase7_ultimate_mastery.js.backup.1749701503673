    /**
     * Log ultimate mastery activities
     */
    logActivity(level, message, context = {}) {
        const logEntry = {
            timestamp: new Date().toISOString(),
            level: level,
            message: message,
            context: context,
            phase: 'Phase 7',
            component: 'Ultimate Mastery',
            achievement_level: 'transcendent'
        };

        // Log to ultimate mastery specific log file
        const logFile = 'logs/mezbjen_ultimate_mastery.log';
        if (!fs.existsSync(path.dirname(logFile))) {
            fs.mkdirSync(path.dirname(logFile), { recursive: true });
        }
        fs.appendFileSync(logFile, JSON.stringify(logEntry) + '\n');
    }
